{
  "name": "graphql-ts-example",
  "version": "1.0.0",
  "description": "This is an example of app using Typescript and GraphQL.",
  "main": "./dist/src/server.js",
  "type": "module",
  "scripts": {
    "compile": "tsc",
    "lint": "eslint",
    "start": "npm run compile && node ./dist/server.js",
    "dev": "nodemon --delay 300ms --exec node --loader ts-node/esm src/server.ts",
    "migration:up": "npx sequelize-cli db:migrate",
    "migration:down": "npx sequelize-cli db:migrate:undo",
    "migration:new": "sequelize-cli migration:generate --name",
    "seed:new": "sequelize-cli seed:generate --name",
    "seed:up": "sequelize-cli db:seed:all --debug",
    "seed:down": "sequelize-cli db:seed:undo"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.3.0",
    "dotenv": "^16.0.3",
    "eslint-friendly-formatter": "^4.0.1",
    "graphql": "^16.6.0",
    "sequelize": "^6.28.0",
    "sequelize-typescript": "^2.1.5",
    "sqlite3": "^5.1.4",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-standard-with-typescript": "^26.0.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^0.55.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.5.2",
    "ts-node": "^10.9.1"
  }
}
